public Id insertCourse(String name, String details, Integer credits) {
    // code here
    apxio__Course__c cource = new apxio__Course__c(
        Name = name;
        apxio__Course_Details__c = details;
    );
    if(credits<1 || credits>4) {return null;}
    else {cource.apxio__Credits__c = String.valueOf(credits);}
    insert cource;
    return cource.Id;
}
