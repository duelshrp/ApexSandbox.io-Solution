public void linkParent(Case c1, Case c2) {
    //code here
    if(c1.ContactId == null || c2.ContactId == null) return;
    if(c1.ContactId == c2.ContactId){
        if(c1.CreatedDate == c2.CreatedDate) return;
        else if(c1.CreatedDate < c2.CreatedDate) c2.ParentId = c1.Id;
        else c1.ParentId = c2.Id;
    }
}