public List<Account> getAccounts(List<Account> accounts) {
    accounts.sort();
    return accounts;
}
// write extra class here 
Public Class AccWrapper implements Comparable{
    public Account acc;
    public AccWrapper(Account acc){
        if(acc == null){
            Exception e = new NullPointerException();
            e.setMessage('Account argument cannot be null');
            throw e;
        }
    }

    Public Integer compareTo(Object compAcc){
        AccWrapper compToAcc = (AccWrapper)compAcc;
        Integer returnInt =0;
        if(acc.AnnualRevenue == compToAcc.acc.AnnualRevenue) returnInt =0;
        else if(acc.AnnualRevenue < compToAcc.acc.AnnualRevenue) returnInt =-1;
        else if(acc.AnnualRevenue > compToAcc.acc.AnnualRevenue) returnInt = 1;
        return returnInt;
    }
}
