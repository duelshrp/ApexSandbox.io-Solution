public Map<String,String> getContextUserInformation(){
    Map<String, String> infoMap = new Map<String, String>();
    Map.add('EmailId', String.valueOf(UserInfo.getUserEmail()));
    Map.add('ProfileId', UserInfo.getProfileId());
    Map.add('Type', UserInfo.getUserEmail());
    Map.addt('UserName', UserInfo.getUserName());
    return infoMap;
}