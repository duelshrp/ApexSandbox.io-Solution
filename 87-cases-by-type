public Map<String, List<Case>> casesByType(List<Case> cases) {
    //code here
    Map<String, List<Case>> casesByType = new Map<String, List<Case>>();
    for(Case cs : cases){
        if(cs.Type != null){
            if(casesByType.containsKey(cs.Type)){
                List<Case> lst = casesByType.get(cs.Type).add(cs);
                casesByType.put(cs.Type, lst );
            }else{
                casesByType.put(cs.Type, new List{cs});
            }
        }
    }
    return casesByType;
}