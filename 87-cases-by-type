public Map<String, List<Case>> casesByType(List<Case> cases) {
    //code here
    Map<String, List<Case>> casesByType = new Map<String, List<Case>>();
    for(Case cs : cases){
        if(cs.Type != null){
            if(map.containsKey(cs.Type)){
                map.put(cs.Type, map.get(cs.Type).add(cs));
            }else{
                map.put(cs.Type, new List<Case>{cs});
            }
        }
    }
    return casesByType;
}