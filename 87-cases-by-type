public Map<String, List<Case>> casesByType(List<Case> cases) {
    //code here
    Map<String, List<Case>> mp = new Map<String, List<Case>>();
    for(Case cs : cases){
        if(cs.Type != null){
            if(mp.containsKey(cs.Type)){
                mp.get(cs.Type).add(cs);
            }else{
                mp.put(cs.Type, new List{cs});
            }
        }
    }
    return mp;
}