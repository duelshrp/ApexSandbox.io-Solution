public Map<String, List<Case>> casesByType(List<Case> cases) {
    //code here
    Map<String, List<Case>> mp = new Map<String, List<Case>>();

    for(Case c : cases){
        if(c.Type != null){
            if(mp.containsKey(c.Type)){
                mp.get(c.Type).add(c);
            }else{
                List<Case> caseList = new List<Case>();
                caseList.add(c)
                mp.put(c.Type, caseList);
            }
        }
    }
    return mp;
}