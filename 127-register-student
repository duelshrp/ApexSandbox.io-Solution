public String registerStudent(String name, String phone, String email) {
    // code here
    apxio__Student__c student = new apxio__Student__c(
        Name = name,
        apxio__Phone__c = phone,
        apxio__Email__c = email
    );
    insert student;
apxio__Student__c stu = [select Id, apxio__Registration_Number__c from apxio__Student__c where Id = student.Id];
    return String.valueOf(stu.apxio__Registration_Number__c);
}